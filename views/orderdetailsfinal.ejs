    <h2 class="primary-heading">Order Details</h2>
    <div class="container outerContainer">

       
          <div class="container text-center cardContainer d-flex ">
            <table class="table caption-top">
              <div class="addcontainer">
              
               
              </div>
              <thead>
                <tr>
                  <th scope="col">S.NO</th>
                  <th scope="col">PRODUCT NAME</th>
                  <th scope="col">PRODUCT COUNT</th>
                  <!-- <th scope="col">PRODUCT PRICE</th> -->
            
               
                         
                </tr>
              </thead>
            
              <tbody id="productTableBody">
                <% let num = 1 ;%>   
                <% order.productsInfo.forEach((element) => { %>
                  <th scope="row"><%=num%></th>
                  <td>
                    <div class="productname">
                
                        <p><%= element.productName %></p>
                 
                    </div>
                  </td>
                  
                  <td>
                    <div class="productname">
                      <!-- <img
                        class="productimg"
                        src=""
                        alt=""
                      /> -->
        
                      <p><%=element.count%></p>
                      
              
                    </div>
                  </td>
                   <!-- <td>
                    <div class="productname">
                       <img
                        class="productimg"
                        src=""
                        alt=""
                      />                   
                      <p><%=element.productprices%></p>
                      
     
                    </div>
                  </td>  -->
    
                </tr>
                <% num = num+1%>
                <% }); %>
              </tbody>
               
            </table>
           
            </div>
          
        
      
      

<div>
  <h5 class="price-heading"> DETAILS</h5>
          <p class="pricedetailsp"><strong>User Name: </strong><span id="price"> <%=order.userName%></span></p>
          <p class="pricedetailsp"><strong>Date : </strong><span id="price"><%= order.Date.toLocaleDateString() %></span></p>
          <p class="pricedetailsp"><strong>Delivery Charge :</strong> <span id="deliveryCharge">$500</span>   </p>
          <p  class="pricedetailsp"><strong>TotalPrice :</strong><span id="totalPrice">$<%=order.totalPrice %></span>  </p>
          <p  class="pricedetailsp"><strong>Address :</strong><span id="address"><%=order.addressdetails.address %></span>  </p>
          <p  class="pricedetailsp"><strong>Pin Code :</strong><span id="pincode"><%=order.addressdetails.postalCode %></span>  </p>
          <p  class="pricedetailsp"><strong>Contact Number :</strong><span id="contact-num"><%=order.addressdetails.contactNumber %></span>  </p>
                <a href="/user/" class="btn btn-Buynow">Back To Shopping</a>
          </p>
</div>
